{
  "name": "nodejs-prompt-cli",
  "version": "0.0.3",
  "description": "A simple package that prints a question to the console and waits for the user's response.",
  "main": "./build/main.js",
  "types": "./build/main.d.ts",
  "scripts": {
    "test": "echo \"crea test pls\"",
    "dev": "nodemon ./src/main.ts",
    "lint": "ts-standard",
    "tsc": "tsc",
    "start": "node build/build/main.js",
    "build:watch": "tsc --build tsconfig.json --watch",
    "clean": "make clean",
    "build": "tsc --build tsconfig.json",
    "build:bundle": "tsc --build tsconfig.json && esbuild ./build/main.js --bundle --outfile=./build/bundle/main.js --minify --allow-overwrite --platform=node --target=node16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisRol2/node-propt-cli.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "cli",
    "prompt",
    "question",
    "console",
    "input",
    "output",
    "readline",
    "readline-sync",
    "readline-promise",
    "readline-promise-ts",
    "readline-promise-ts-cli",
    "readline-promise-ts-cli-prompt",
    "readline-promise-ts-cli-prompt-question"
  ],
  "author": "ChrisRol",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chrisRol2/node-propt-cli/issues"
  },
  "homepage": "https://github.com/chrisRol2/node-propt-cli",
  "dependencies": {
    "dotenv": "^16.0.1",
    "readline": "1.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "esbuild": "^0.15.7",
    "nodemon": "2.0.16",
    "ts-node": "10.8.0",
    "ts-node-dev": "1.1.8",
    "ts-standard": "11.0.0",
    "typescript": "4.7.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "./node_modules/ts-standard/config/ts-standard.json"
    ]
  }
}
